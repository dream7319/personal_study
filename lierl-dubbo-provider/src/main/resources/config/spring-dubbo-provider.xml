<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="lierl-dubbo-provider"/>
    <dubbo:registry address="multicast://224.5.6.8:1235"/>
    <dubbo:protocol name="dubbo" port="20880"/>
    <dubbo:service interface="com.lierl.dubbo.service.DemoService" ref="demoServiceImpl"/>
    <bean id="demoServiceImpl" class="com.lierl.dubbo.service.impl.DemoServiceImpl"/>
</beans>